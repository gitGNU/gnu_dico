! $Id: Gjdict.ad,v 1.1 2001/11/03 22:36:43 gray Exp $
Gjdict.title: GNU Japanese Dictionary
!
*background: gray
*highlightBackground: white
*highlightForeground: black
*server: localhost
*port: gjdict
*largeKanjiFont: kanji24
*smallKanjiFont: kanji16
*bushuFont: -gray-Fixed-Medium-R-Normal--16-150-75-75-C-160-bushu.5.1999-0
*englishFont:     fixed
*pinyin: True
*romaji: False
*verbose: False
*dictBufferSize: 2048
*dictBufferIncr: 128
*dictEnglishFormat: NumberedNewLines
*HistorySize: 32
*helpfile: /home/gray/gjdict/client/gjdict.chl
!
!
*sink.kanjiFont: -jis-*-*-*-*-*-*-*-75-75-*-*-*-*
*sink.font:-sony-*-*-*-*-*-*-*-75-75-*-*-*-*
*source.type: string
*source.encoding: euc
*source.useStringInPlace: True
!
*menuForm.width: 493
*menuForm*shadowWidth: 0
*menuForm.borderWidth: 1
*commandMenuButton.label: Command
*commandMenu.connect.label: Connect
*commandMenu.quit.label: Quit
!
*optionsMenuButton.label: Options
!*optionsMenuButton.horizDistance: 50
*optionsMenu.edit.label: Edit
*optionsMenu.save.label: Save
!
*helpMenuButton.label: Help
!*helpMenuButton.horizDistance: 200
*helpMenu.help.label: Help
*helpMenu.license.label: License
!
*buttonForm.width: 400
*buttonForm.borderWidth: 0
*buttonForm.*.borderWidth: 0
*buttonForm.next.label: Next
*buttonForm.prev.label: Prev
*buttonForm.findNext.label: Find next
*buttonForm.findPrev.label: Find prev
!
*status.label: Ready 
*status.width: 493
!
! Connect shell
*connectShell.rel.x: 100
*connectShell.rel.y: 100
*connectShell.connectForm.serverLabel.label: Server
*connectShell.connectForm.serverInput.width: 200
*connectShell.connectForm.portLabel.label: Port
*connectShell.connectForm.portInput.width: 50
*connectShell.connectForm.ok.label: Connect
*connectShell.connectForm.cancel.label: Cancel
!
*box.Toggle.shadowWidth: 0
! Help shell
*helpShell.title: Gjdict Online Help
*helpShell.rel.x: 10
*helpShell.rel.y: -40
*helpForm*font: -sony-*-*-r-*-*-*-*-75-75-*-*-*-*
*helpForm.topic.label: Select a topic
*helpForm.topicList.box*width: 250
*helpForm.topicList.height: 300
*helpForm.topicTitle.width: 500
*helpForm.topicText.width: 500
*helpForm.topicText.height: 200
*helpForm.crossRefTitle.label: See also
*helpForm.crossRef.box*width: 400
*helpForm.crossRef.height: 100
*helpForm.close.label: Close
*helpForm.back.label: Back
*helpForm.forward.label: Forward
*helpForm.history.label: History
!
*helpHistoryShell.title: Help history list
*helpHistoryShell.rel.x: 10
*helpHistoryShell.rel.y: -40
*helpHistoryForm*font: -sony-*-*-r-*-*-*-*-75-75-*-*-*-*
*helpHistoryForm.historyList.box*width: 250
*helpHistoryForm.historyList.height: 200
*helpHistoryForm.close.label: Close
*helpHistoryForm.select.label: Select
!
! License shell
*licenseShell.rel.x: 50
*licenseShell.rel.y: 50
*licenseShell.form.info.borderWidth: 0
*licenseShell.form.close.label: Close
*licenseShell.form.close.fromVert: info

